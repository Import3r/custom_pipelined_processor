module hazard_detection_unit(Load_signal,rs,rt,rd,ID_EXE_rt,ID_EXE_MemRead,op1_selector,op2_selector,PC_write,IF_ID_write,flush);
input Load_signal, ID_EXE_MemRead;
input [1:0] op1_selector,op2_selector;
input [4:0] rs,rt,rd,ID_EXE_rt;
output PC_write, IF_ID_write, flush;
reg [4:0] OP1,OP2;
always @* begin
case(op1_selector)
2'b00: OP1 = rs;
2'b01: OP1 = rt;
2'b10: OP1 = rd;
end
case(op2_selector)
2'b00: OP2 = rs;
2'b01: OP2 = rt;
2'b10: OP2 = rd;
end
if(Load_signal == 1'b1) begin

end
end
endmodule